<!DOCTYPE html>
<html lang="en">
<head>

    <base href="~/" />

    <title>Profile Page - About</title>

    <!-- Required meta tags always come first -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" type="text/css" asp-href-include="Bootstrap/dist/css/bootstrap-reboot.css">
    <link rel="stylesheet" type="text/css" asp-href-include="Bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" asp-href-include="Bootstrap/dist/css/bootstrap-grid.css">

    <!-- Main Styles CSS -->
    <link rel="stylesheet" type="text/css" asp-href-include="css/main.min.css">
    <link rel="stylesheet" type="text/css" asp-href-include="css/fonts.min.css">

    <!-- Main Font -->
    <script asp-src-include="js/webfontloader.min.js"></script>
    <script>
        WebFont.load({
            google: {
                families: ['Roboto:300,400,500,700:latin']
            }
        });
    </script>

</head>
<body>
    @await Component.InvokeAsync("SideBarLeftComponent");
    @await Component.InvokeAsync("SideBarRightComponent");
    @await Component.InvokeAsync("TopHeaderComponent");
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <!-- JS Scripts -->
    <script asp-src-include="js/jquery-3.2.1.js"></script>
    <script asp-src-include="js/jquery.appear.js"></script>
    <script asp-src-include="js/jquery.mousewheel.js"></script>
    <script asp-src-include="js/perfect-scrollbar.js"></script>
    <script asp-src-include="js/jquery.matchHeight.js"></script>
    <script asp-src-include="js/svgxuse.js"></script>
    <script asp-src-include="js/imagesloaded.pkgd.js"></script>
    <script asp-src-include="js/Headroom.js"></script>
    <script asp-src-include="js/velocity.js"></script>
    <script asp-src-include="js/ScrollMagic.js"></script>
    <script asp-src-include="js/jquery.waypoints.js"></script>
    <script asp-src-include="js/jquery.countTo.js"></script>
    <script asp-src-include="js/popper.min.js"></script>
    <script asp-src-include="js/material.min.js"></script>
    <script asp-src-include="js/bootstrap-select.js"></script>
    <script asp-src-include="js/smooth-scroll.js"></script>
    <script asp-src-include="js/selectize.js"></script>
    <script asp-src-include="js/swiper.jquery.js"></script>
    <script asp-src-include="js/moment.js"></script>
    <script asp-src-include="js/daterangepicker.js"></script>
    <script asp-src-include="js/simplecalendar.js"></script>
    <script asp-src-include="js/fullcalendar.js"></script>
    <script asp-src-include="js/isotope.pkgd.js"></script>
    <script asp-src-include="js/ajax-pagination.js"></script>
    <script asp-src-include="js/Chart.js"></script>
    <script asp-src-include="js/chartjs-plugin-deferred.js"></script>
    <script asp-src-include="js/circle-progress.js"></script>
    <script asp-src-include="js/loader.js"></script>
    <script asp-src-include="js/run-chart.js"></script>
    <script asp-src-include="js/jquery.magnific-popup.js"></script>
    <script asp-src-include="js/jquery.gifplayer.js"></script>
    <script asp-src-include="js/mediaelement-and-player.js"></script>
    <script asp-src-include="js/mediaelement-playlist-plugin.min.js"></script>

    <script asp-src-include="js/base-init.js"></script>
    <script defer asp-src-include="fonts/fontawesome-all.js"></script>

    <script asp-src-include="Bootstrap/dist/js/bootstrap.bundle.js"></script>
</body>
</html>
